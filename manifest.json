{
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Whatsapp number collector"
  },
  "description": "Collect all messages from a web.whatsapp.com that were not saved",
  "icons": {
    "128": "images/whatsapp-collector-logo128.png",
    "16": "images/whatsapp-collector-logo16.png",
    "32": "images/whatsapp-collector-logo32.png",
    "48": "images/whatsapp-collector-logo48.png"
  },
  "manifest_version": 2,
  "name": "Whatsapp number collector",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "downloads",
    "tabs",
    "*://*.whatsapp.com/*",
    "contextMenus",
    "notifications",
    "unlimitedStorage",
    "storage"
  ],
  "short_name": "Whatsapp number collector",
  "version": "1.1"
}
