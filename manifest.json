{
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Planex Recolector WhatsApp"
  },
  "description": "Exporta todos los n√∫meros no guardados",
  "icons": {
    "128": "images/get_started128.png",
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png"
  },
  "manifest_version": 2,
  "name": "Planex Recolector WhatsApp",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "downloads",
    "tabs",
    "*://*.whatsapp.com/*",
    "contextMenus",
    "notifications",
    "unlimitedStorage",
    "storage"
  ],
  "short_name": "Planex Recolector WhatsApp",
  "version": "1.1"
}
